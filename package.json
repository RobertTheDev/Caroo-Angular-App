{
  "name": "caroo-angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run caroo-angular-app:serve-ssr",
    "serve:ssr": "node dist/caroo-angular-app/server/main.js",
    "build:ssr": "ng build && ng run caroo-angular-app:server",
    "prerender": "ng run caroo-angular-app:prerender",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint \"src/app/**/*.css\"",
    "prettier:check": "prettier --check --ignore-path .gitignore .",
    "prettier:write": "prettier --write --ignore-path .gitignore ."
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "src/**/*.{ts,css,html}": [
      "npm run prettier:check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@nguniversal/express-engine": "^15.2.1",
    "@prisma/client": "^5.1.1",
    "express": "^4.15.2",
    "express-session": "^1.17.3",
    "http-status-codes": "^2.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zod": "^3.22.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.0.3",
    "@angular/compiler-cli": "^15.0.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "commitlint": "^17.7.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "prettier-eslint": "^15.0.1",
    "prisma": "^5.1.1",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "~4.8.2"
  }
}
